graph {
splines=spline;
overlap=false;
"AnAggregateRootId" -- "AnotherAggregate" [style=solid];
"AnAggregate" -- "AnAggregateRootId" [style=dotted];
"AnEnum" -- "AnotherAggregate" [style=solid];
